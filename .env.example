# Firebase Configuration
# Copy this file to .env and fill in your Firebase project details

PLASMO_PUBLIC_FIREBASE_API_KEY=your-api-key-here
PLASMO_PUBLIC_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
PLASMO_PUBLIC_FIREBASE_PROJECT_ID=your-project-id
PLASMO_PUBLIC_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
PLASMO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=123456789
PLASMO_PUBLIC_FIREBASE_APP_ID=1:123456789:web:abcdef

# Optional: Enable Firebase emulator for development
# PLASMO_PUBLIC_USE_FIREBASE_EMULATOR=true
